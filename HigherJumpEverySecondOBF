local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Higher Speed Every Second", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

local Tab1 = Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Eggs",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab3 = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

---------------------------------------

_G.autoFarm1 = true

function autoFarm1()
	while _G.autoFarm1 == true do
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-392.799988, 60, -2875, 0, 0, -1, 0, 1, 0, 1, 0, 0)
	wait(0.3)
	end
end

Tab1:AddToggle({
	Name = "Auto Farm Rainbow Lane | 1 WIN(S)",
	Callback = function(Value)
		_G.autoFarm1 = Value
		autoFarm1()
    end 
})

