_G.autoRebirth = true

function autoRebirth()
	while _G.autoRebirth == true do
		local args = {
			[1] = "S_System_Rebirth",
			[2] = {}
		}
		
		game:GetService("ReplicatedStorage").Common.Library.Network.RemoteFunction:InvokeServer(unpack(args))
	wait(0.3)
	end
end

Tab1:AddToggle({
	Name = "Auto Rebirth",
	Callback = function(Value)
		_G.autoRebirth = Value
		autoRebirth()
    end
})

---------------------------------------

_G.autoOpenEgg1 = true

function autoOpenEgg1()
	while _G.autoOpenEgg1 == true do
        local args = {
            [1] = "S_Egg_Open",
            [2] = {
                [1] = "Candy",
                [2] = false
            }
        }
        
        game:GetService("ReplicatedStorage").Common.Library.Network.RemoteEvent:FireServer(unpack(args))        
	wait(0.3)
	end
end

Tab2:AddToggle({
	Name = "Auto Open Candy Egg",
	Callback = function(Value)
		_G.autoOpenEgg1 = Value
		autoOpenEgg1()
    end 
})

---------------------------------------

